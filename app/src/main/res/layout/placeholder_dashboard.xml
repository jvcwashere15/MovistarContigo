<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".principal.MainActivity">

    <View
        android:id="@+id/placeholder_dashboard_vwNoticias"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/colorPlaceHolder"
        app:layout_constraintBottom_toTopOf="@+id/viewNormal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/imageView3"
        android:layout_width="0dp"
        android:layout_height="160dp"
        android:layout_gravity="top"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.4" />

    <View
        android:id="@+id/home_tvHolaUsuario"
        android:layout_width="160dp"
        android:layout_height="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="@color/colorPlaceHolder"
        android:fontFamily="@font/telefonica_bold"
        android:text="@string/dashboard_beneficios_usuario"
        android:textColor="@android:color/white"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <View
        android:id="@+id/textView13"
        android:layout_width="240dp"
        android:layout_height="16dp"
        android:background="@color/colorPlaceHolder"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/telefonica_regular"
        android:includeFontPadding="false"
        android:text="@string/dashboard_beneficios_descripcion"
        android:textColor="@android:color/white"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_tvHolaUsuario"
        app:layout_constraintVertical_bias="0.0" />


    <View
        android:id="@+id/viewNormal"
        android:layout_width="0dp"
        android:layout_height="1dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.3" />

    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:layout_constraintVertical_bias="1.0">

        <View
            android:id="@+id/dash_cvDescuentos"
            android:layout_width="88dp"
            android:layout_height="88dp"
            android:layout_margin="8dp"
            android:layout_marginBottom="8dp"
            android:background="@color/colorPlaceHolder"
            app:layout_constraintBottom_toTopOf="@+id/textView16"
            app:layout_constraintEnd_toStartOf="@+id/dash_cvSalud"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"/>

        <View
            android:id="@+id/dash_cvSalud"
            android:layout_width="@dimen/lado_item_beneficios_dash"
            android:layout_height="@dimen/lado_item_beneficios_dash"
            android:layout_margin="8dp"
            android:layout_marginEnd="8dp"
            android:background="@color/colorPlaceHolder"
            app:layout_constraintBottom_toTopOf="@+id/textView16"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <View
            android:id="@+id/dash_cvCanalEmbajador"
            android:layout_width="@dimen/lado_item_beneficios_dash"
            android:layout_height="@dimen/lado_item_beneficios_dash"
            android:layout_marginTop="8dp"
            android:background="@color/colorPlaceHolder"
            app:layout_constraintBottom_toBottomOf="@+id/dash_cvSalud"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/dash_cvSalud"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"/>

        <View
            android:id="@+id/textView16"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/dash_cvBeneficiosEspeciales"
            android:layout_width="@dimen/lado_item_beneficios_dash"
            android:layout_height="@dimen/lado_item_beneficios_dash"
            android:layout_margin="8dp"
            android:layout_marginTop="8dp"
            android:background="@color/colorPlaceHolder"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView16"
            app:layout_constraintVertical_bias="0.0" />

        <View
            android:id="@+id/dash_cvEstudios"
            android:layout_width="87dp"
            android:layout_height="@dimen/lado_item_beneficios_dash"
            android:layout_margin="8dp"
            android:layout_marginTop="8dp"
            android:background="@color/colorPlaceHolder"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/dash_cvBeneficiosEspeciales"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView16"
            app:layout_constraintVertical_bias="0.0" />

        <View
            android:id="@+id/dash_cvVacaciones"
            android:layout_width="87dp"
            android:layout_height="@dimen/lado_item_beneficios_dash"
            android:layout_margin="8dp"
            android:layout_marginTop="8dp"
            android:background="@color/colorPlaceHolder"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/dash_cvBeneficiosEspeciales"
            app:layout_constraintTop_toBottomOf="@+id/textView16"
            app:layout_constraintVertical_bias="0.0" />
    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>